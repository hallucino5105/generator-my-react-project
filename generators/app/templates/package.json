{
  "name": "<%= projname %>",
  "version": "1.0.0",
  "description": "",
  "private": true,
  <% if (framework_type === "React") { %>
  "main": "index.js",
  <% } else if (framework_type === "React Native") { %>
  "main": "src/core_main/entry/index.js",
  <% } else if (framework_type === "Electron") { %>
  "main": "index.js",
  <% } %>
  "scripts": {
    <% if (framework_type === "React") { %>
    "__prebuild": "npm run mkdir && npm run link",
    "watch": "webpack --mode development --config ./webpack/webpack.app.config.js -w --progress",
    "build:app:stag": "npm run __prebuild && webpack --mode development --progress --config webpack/webpack.app.config.js",
    "build:app:prod": "npm run __prebuild && webpack --mode production  --progress --config webpack/webpack.app.config.js",
    "build:dll:stag": "npm run __prebuild && webpack --mode development --progress --config webpack/webpack.dll.config.js",
    "build:dll:prod": "npm run __prebuild && webpack --mode production  --progress --config webpack/webpack.dll.config.js",
    "build:app": "npm run build:app:stag",
    "build:dll": "npm run build:dll:stag",
    "build:all:stag": "npm run build:dll:stag && npm run build:app:stag",
    "build:all:prod": "npm run build:dll:prod && npm run build:app:prod",
    "build:all": "npm run build:all:stag",
    "build": "npm run build:app:stag",
    "analyze:app:stag": "npm run __prebuild && webpack --mode development --progress --config webpack/webpack.app.config.js",
    "analyze:app:prod": "npm run __prebuild && webpack --mode production --progress --config webpack/webpack.app.config.js",
    "analyze:dll:stag": "npm run __prebuild && webpack --mode development --progress --config webpack/webpack.dll.config.js",
    "analyze:dll:prod": "npm run __prebuild && webpack --mode production --progress --config webpack/webpack.dll.config.js",
    "analyze:app": "npm run analyze:app:stag",
    "analyze:dll": "npm run analyze:dll:stag",
    "analyze": "npm run analyze:app:stag",
    "release": "npm run build:all:prod",
    "export": "npm run build:prod && ./scripts/export/export.py",
    "init": "npm install --force && npm run build",
    "serve": "webpack-dev-server --mode development --config webpack/webpack.app.config.js",
    "serve:prod": "webpack-dev-server --mode production --config webpack/webpack.app.config.js",
    "start": "concurrently 'npm run app' 'npm run serve'",
    "start:prod": "concurrently 'npm run app' 'npm run serve:prod'",
    "uwsgi": "uwsgi --ini $(pwd)/misc/conf/uwsgi/uwsgi_app.ini --chdir $(pwd)",
    "mkdir": "mkdir -p stag && mkdir -p prod",
    "version": "./scripts/version/version.py",
    "version:update": "./scripts/version/update_version.py",
    "link": "./scripts/link/link.py",
    "module": "./scripts/module/gen_module.py",
    "clean": "./scripts/clean/clean.sh",
    "app": "./backend/app/app.py"
    <% } else if (framework_type === "React Native") { %>
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject"
    <% } else if (framework_type === "Electron") { %>
    <% } %>
  },
  "directories": {
    "test": "test/"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    <% if (framework_type === "React") { %>
    "@blueprintjs/core": "^3.13.0",
    "@material-ui/core": "^3.9.2",
    "classnames": "^2.2.6",
    "core-js": "^2.6.4",
    "deepmerge": "^3.1.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.1",
    "qs": "^6.6.0",
    "react": "~16",
    "react-dom": "~16",
    "react-redux": "^6.0.0",
    "react-responsive": "^6.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "reactotron-react-js": "^2.1.4",
    "reactotron-redux": "^2.1.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "resolve-url-loader": "^3.0.0",
    "socket.io-client": "^2.2.0",
    "sprintf-js": "^1.1.2",
    "style-it": "^2.1.2"
    <% } else if (framework_type === "React Native") { %>
    "expo": "^32.0.0",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz"
    <% } else if (framework_type === "Electron") { %>
    <% } %>
  },
  "devDependencies": {
    <% if (framework_type === "React") { %>
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.0.1",
    "@types/react-responsive": "^3.0.3",
    "@types/redux-logger": "^3.0.7",
    "@types/socket.io-client": "^1.4.32",
    "@types/webpack": "^4.4.25",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "blueprint-css": "^3.1.0",
    "bufferutil": "^4.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "concurrently": "^4.1.0",
    "core-js": "^2.6.4",
    "css-loader": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "glsl-shader-loader": "^0.1.6",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "intelli-espower-loader": "^1.0.1",
    "json-loader": "^0.5.7",
    "mocha": "^6.0.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "power-assert": "^1.6.1",
    "react-hot-api": "^0.5.0",
    "react-hot-loader": "^4.6.5",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "utf-8-validate": "^5.0.2",
    "webpack": "^4.29.3",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.7.0"
    <% } else if (framework_type === "React Native") { %>
    "babel-preset-expo": "^5.0.0"
    <% } else if (framework_type === "Electron") { %>
    <% } %>
  }
}
